                    <div class="container">
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">

                             
                                <% likedofertauno.forEach(function (ofertaun) { %>
                                <!--====== Wishlist Product ======-->
                                <div class="p">
                                <div class="w-r u-s-m-b-30"  data-id="<%= ofertaun.id %>">
                                    <div class="w-r__container">
                                        <div class="w-r__wrap-1">
                                            <div class="w-r__img-wrap">

                                                <img class="u-img-fluid" src="<%= ofertaun.image %>" alt=""></div>
                                            <div class="w-r__info">

                                                <span class="w-r__name">

                                                    <a><%= ofertaun.title %></a></span>

                                                <span class="w-r__category">

                                                    <a>   </a></span>

                                                <span class="w-r__price">$<%= ofertaun.price %>

                                                    <span class="w-r__discount">     </span></span></div>
                                        </div>
                                        <div class="w-r__wrap-2">

                                            <a 
                                            href="/addtocardofertauno/<%= ofertaun.id %>" 
                                            class="w-r__link btn--e-brand-b-2"  >
                                            AGREGAR AL CARRO</a>

                                            <a 
                                            class="w-r__link btn--e-transparent-platinum-b-2" 
                                            href="/ofertaunoredirect/<%= ofertaun.id %>">
                                            VER</a>

                                            <a 
                                            class="w-r__link btn--e-transparent-platinum-b-2 fas fa-trash" 
                                            href="/like/<%= ofertaun.id %>"  
                                            id="unlike-uno"  data-id="<%= ofertaun.id %>"
                                            ></a></div>
                                    </div>
                                </div>
                            </div>
                                <!--====== End - Wishlist Product ======-->
                                <% }) %>



